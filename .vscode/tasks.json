{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run DB Test Migrations",
			"type": "shell",
			"command": "npm run migrate:test",
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "Install dev deps for migrations",
			"type": "shell",
			"command": "npm install --save-dev node-pg-migrate",
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		}
	]
}